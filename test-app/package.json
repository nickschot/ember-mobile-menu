{
  "name": "test-app",
  "version": "0.0.0",
  "private": true,
  "description": "Test app for ember-mobile-menu addon",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "vite",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "vite build --mode development && ember test --path dist"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/eslint-parser": "7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@ember/optional-features": "2.2.0",
    "@ember/string": "4.0.1",
    "@ember/test-helpers": "5.4.0",
    "@embroider/compat": "^4.0.3",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.2.5",
    "@embroider/test-setup": "4.0.0",
    "@embroider/vite": "^1.1.1",
    "@eslint/js": "9.39.1",
    "@glimmer/component": "2.0.0",
    "@glimmer/tracking": "1.1.2",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "2.11.2",
    "ember-cli": "6.8.0",
    "ember-cli-babel": "8.2.0",
    "ember-cli-htmlbars": "6.3.0",
    "ember-functions-as-helper-polyfill": "2.1.3",
    "ember-load-initializers": "3.0.1",
    "ember-mobile-menu": "workspace:*",
    "ember-modifier": "4.2.2",
    "ember-qunit": "^9.0.0",
    "ember-resolver": "13.1.1",
    "ember-source": "6.8.1",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "7.9.3",
    "ember-try": "4.0.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-ember": "12.7.4",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-qunit": "8.2.5",
    "globals": "16.5.0",
    "prettier": "3.6.2",
    "prettier-plugin-ember-template-tag": "2.1.0",
    "qunit": "2.24.2",
    "qunit-dom": "3.5.0",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-prettier": "5.0.3",
    "tracked-built-ins": "4.0.0",
    "vite": "^7.2.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
