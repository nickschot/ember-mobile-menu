    "tracked-maps-and-sets": "^3.0.0",
    "wobble": "^1.5.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "ember-source": "4.7.0",
    "ember-template-lint": "4.14.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "11.0.6",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "webpack": "5.74.0"
  },
  "peerDependencies": {
    "ember-source": "*"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {},
  "homepage": "https://nickschot.github.io/ember-mobile-menu",
  "volta": {
    "extends": "../package.json"
  }
