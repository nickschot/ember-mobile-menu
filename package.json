{
  "name": "ember-mobile-menu",
  "version": "2.1.1",
  "description": "A draggable sidebar menu for Ember.",
  "keywords": [
    "ember-addon",
    "mobile",
    "menu",
    "sidebar",
    "hamburger-menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nickschot/ember-mobile-menu"
  },
  "license": "MIT",
  "author": "Nick Schot <nickschot@webhub.nl>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "npx lerna-changelog --from=v0.1.0-alpha.5 > CHANGELOG.md",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "node ./compile-css.js"
  },
  "dependencies": {
    "@ember/legacy-built-in-components": "^0.4.0",
    "@ember/render-modifiers": "^2.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "body-scroll-lock": "^3.0.1",
    "ember-auto-import": "^2.2.4",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-concurrency": "^2.1.0",
    "ember-could-get-used-to-this": "^1.0.0",
    "ember-gesture-modifiers": "^2.0.0",
    "ember-on-resize-modifier": "^0.4.0",
    "ember-set-body-class": "^1.0.1",
    "tracked-maps-and-sets": "^3.0.0",
    "wobble": "^1.5.1"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.6.0",
    "@embroider/test-setup": "1.5.0",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "4.2.0",
    "ember-cli-addon-docs": "4.2.2",
    "ember-cli-addon-docs-yuidoc": "1.0.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-deploy": "1.0.2",
    "ember-cli-deploy-build": "2.0.0",
    "ember-cli-deploy-git": "1.3.4",
    "ember-cli-deploy-git-ci": "1.0.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sass": "10.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-data": "4.3.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-page-title": "7.0.0",
    "ember-qunit": "5.1.5",
    "ember-resize-observer-polyfill": "0.0.1",
    "ember-resolver": "8.0.3",
    "ember-sinon": "5.0.0",
    "ember-sinon-qunit": "6.0.0",
    "ember-source": "4.1.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "4.0.0",
    "ember-try": "2.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-ember": "10.5.8",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-qunit": "7.2.0",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "qunit": "2.17.2",
    "qunit-dom": "2.0.0",
    "sass": "1.44.0",
    "webpack": "5.71.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "homepage": "https://nickschot.github.io/ember-mobile-menu",
  "resolutions": {
    "nwsapi": "^2.1.0",
    "ember-href-to": "^5.0.0",
    "ember-keyboard": "^7.0.0-beta.0"
  },
  "volta": {
    "node": "16.13.2"
  }
}
