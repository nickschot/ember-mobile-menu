{
  "version": "5.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nickschot/ember-mobile-menu"
  },
  "workspaces": [
    "ember-mobile-menu",
    "docs",
    "test-app"
  ],
  "scripts": {
    "build": "pnpm --filter ember-mobile-menu build",
    "changelog": "npx lerna-changelog --from=v0.1.0-alpha.5 > CHANGELOG.md",
    "lint": "pnpm --filter '!docs' lint",
    "lint:fix": "pnpm --filter '!docs' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefixColors auto",
    "start:addon": "pnpm --filter ember-mobile-menu start --no-watch.clearScreen",
    "start:docs": "pnpm --filter docs start",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test",
    "test:ember": "pnpm --filter '*' test:ember"
  },
  "pnpm": {
    "overrides": {
      "@handlebars/parser": "2.0.0"
    },
    "patchedDependencies": {
      "ember-app-scheduler": "patches/ember-app-scheduler.patch"
    },
    "packageExtensions": {
      "ember-app-scheduler": {
        "dependencies": {
          "@ember/test-waiters": "*",
          "ember-auto-import": "*"
        }
      },
      "ember-tether": {
        "peerDependencies": {
          "ember-source": "*"
        }
      },
      "@ember/render-modifiers": {
        "peerDependencies": {
          "ember-source": "*"
        }
      }
    }
  },
  "devDependencies": {
    "concurrently": "9.1.2",
    "prettier": "3.5.3",
    "prettier-plugin-ember-template-tag": "2.0.5",
    "release-plan": "0.16.0"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
